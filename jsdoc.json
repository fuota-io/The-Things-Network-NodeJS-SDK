{
  "source": {
    "include": ["dist/User/index.js", "dist/Organization/index.js", "dist/Application/index.js", "dist/Device/index.js", "dist/index.js",
   "dist/Interfaces/Doc Common/doc.interface.js"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "recurse": true,
    "destination": "./docs/",
    "encoding": "utf8",
    "verbose": true,
    "theme_opts": {
      "default_theme": "dark",
      "search": false,
      "includeFilesListInHomepage": false
  }  
},
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
}
}